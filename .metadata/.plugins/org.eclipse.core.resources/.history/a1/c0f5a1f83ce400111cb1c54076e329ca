package test;

import static org.junit.Assert.*;

import org.junit.Test;

import dominio.Cliente;

public class Clientetest {

	
	@Test
	public void TestCliente(){
		
		
		Cliente cliente1 = new Cliente();
		
	cliente1.setDni("45538817");
	cliente1.setNombres("Eduardo");
	cliente1.setApellido_Paterno("Rodriguez");
	cliente1.setApellido_Materno("Valladares");
	cliente1.setTelefono("4984384");
	cliente1.setFechaPase("05/02/2012");
	cliente1.setCorreo("tito.rodriguev@hotmail.com");
	cliente1.setDireccion("callao n4 lote");
	cliente1.setDistrito("Bellavista");
	cliente1.setDepartamento("Callao");
	

	assertEquals("45538817", cliente1.getDni());
	assertEquals("Eduardo", cliente1.getNombres());
	assertEquals("Rodriguez", cliente1.getApellido_Paterno());
	assertEquals("Valladares", cliente1.getApellido_Materno());
	assertEquals("4984384", cliente1.getTelefono());
	assertEquals("05/02/2012", cliente1.getFechaPase());
	assertEquals("tito.rodriguev@hotmail.com", cliente1.getCorreo());
	assertEquals("callao n4 lote", cliente1.getDireccion());
	assertEquals("Bellavista", cliente1.getDistrito());
	assertEquals("Callao", cliente1.getDepartamento());
		
	
	}
}	



